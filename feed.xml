---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title }}</title>
    <description>{{ site.description }}</description>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/feed.xml" rel="self" type="application/rss+xml" />
    {% for post in collections.post %}
      <item>
        <title>{{ post.data.title }}</title>
        <description>{{ post.templateContent | fifty_words }}</description>
        <pubDate>{{ post.date | date: "%d %b %Y" }}</pubDate>
        <link>https://arbeeco.de{{ post.url }}</link>
        <author>{{ post.data.author }}</author>
        <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
        <image>
          <url>https://arbeeco.de{{ post.data.authorimage }}</url>
          <title>authorimage</title>
          <link>{{ post.data.authorurl }}</link>
        </image>
        <media:content medium="image" url="{{ post.data.postimage }}"/>
      </item>
    {% endfor %}
  </channel>
</rss>